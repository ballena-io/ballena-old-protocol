  "license": "MIT",
  "devDependencies": {
    "@openzeppelin/contracts": "^3.4.0",
    "@truffle/hdwallet-provider": "^1.2.2",
    "@typechain/truffle-v5": "^4.0.1",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "ganache-cli": "^6.12.2",
    "rimraf": "^3.0.2",
    "truffle": "^5.1.66",
    "truffle-plugin-verify": "^0.5.5",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.2",
    "typescript": "^4.2.2"
  }
}
